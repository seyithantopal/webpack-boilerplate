{
	"name": "webpack_babel",
	"version": "1.0.0",
	"description": "Webpack 4 boilerplate using Babel, ESLint and SASS with optimized production build.",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --open --config config/webpack.dev.js",
		"build": "webpack --config config/webpack.prod.js",
