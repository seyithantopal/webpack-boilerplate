	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^4.3.0",
		"eslint": "^7.11.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.21.4",
		"eslint-webpack-plugin": "^2.1.0",
		"file-loader": "^6.1.1",
		"html-loader": "^1.3.2",
